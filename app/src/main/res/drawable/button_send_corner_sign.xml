<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="@color/cardview_light_background"
                    android:dashWidth="1dp" android:dashGap="0dp"/>
            <padding android:left="9dp" android:top="2dp"
                     android:right="9dp" android:bottom="2dp"/>
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp"
                    android:dashWidth="1dp" android:dashGap="0dp"/>
            <padding android:left="9dp" android:top="2dp"
                     android:right="9dp" android:bottom="2dp"/>
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="#f1f1f1"

                    android:dashWidth="1dp" android:dashGap="0dp" />
            <corners android:radius="20dp" />
        </shape>
    </item>
</selector>